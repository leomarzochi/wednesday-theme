				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#5b42ad"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
